{
    "name": "mvqn-ucrm/plugins",
    "description": "A library of helper classes to ease UCRM Plugin development.",
    "minimum-stability": "stable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Spaeth",
            "email": "rspaeth@mvqn.net"
        }
